# Environment configuration for the RAG pipeline
# Safe defaults and placeholders. Do not quote values.

# Prevent Transformers from importing torchvision (avoids vision deps for text models)
TRANSFORMERS_NO_TORCHVISION=1

# Qdrant connection (fill these before running ingestion)
QDRANT_URL="Your_URL"
QDRANT_API_KEY="Your_Key"
QDRANT_COLLECTION=gpu_llm_docs

# Default path to the embeddings JSONL (used by rag.qdrant_ingest if --in not provided)
EMBEDDINGS_JSONL=data/chunks/miniLM_embeddings.jsonl

